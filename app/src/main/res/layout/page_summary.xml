<?xml version="1.0" encoding="utf-8"?>
<pl.pharmaway.prezentacjatrilac.view.FixedScaledLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:fixedWidth="1920dp"
    app:fixedHeight="1200dp"
    >

    <pl.pharmaway.prezentacjatrilac.view.FixedImageView
        android:layout_width="1920dp"
        android:layout_height="1200dp"
        app:imageReference="@drawable/p19"
        android:id="@+id/all"
        />

    <View
        android:layout_width="980dp"
        android:layout_height="650dp"
        android:layout_gravity="left"
        android:id="@+id/trilac"
        />

    <View
        android:layout_width="900dp"
        android:layout_height="650dp"
        android:layout_gravity="right"
        android:id="@+id/ecomer"
        />


    <View
        android:layout_width="840dp"
        android:layout_height="550dp"
        android:layout_gravity="center_horizontal|bottom"
        android:id="@+id/aquamer"
        />


    <include
        layout="@layout/footer" />
</pl.pharmaway.prezentacjatrilac.view.FixedScaledLayout>